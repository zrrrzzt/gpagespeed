  },
  "homepage": "https://github.com/zrrrzzt/gpagespeed#readme",
  "dependencies": {
    "googleapis": "30.0.0",
    "valid-url": "1.0.9"
  },
  "devDependencies": {
    "coveralls": "3.0.2",
    "nsp": "3.2.1",
    "standard": "11.0.1",
    "tap": "12.0.1"
  }
